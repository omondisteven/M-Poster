{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(poster|settings|business-profile)",
      "destination": "/index.html"
    },
    {
      "source": "/(poster|settings|business-profile)/(.*)",
      "destination": "/index.html"
    }
  ]
}
